module AUTH1_M

  -- Set of all facts that would be renamed to signals in infer events
  FACTS_TO_RENAME = 
    {Sent.(Encrypt.(PK__.(b), <nb>), <a, b_2_, na, nb_2_>) | 
      Sent.(Encrypt.(PK__.(b), <nb>), <a, b_2_, na, nb_2_>) <- INTRUDER_M::KnowableFact}

  -- Set of tuples of the form (Fact, RunningFact) that is used to create a
  -- renaming function of type Fact -> RunningFact.
  FACT_RENAMING =
    Union({
      {(Sent.(Encrypt.(PK__.(b), <nb>), <a, b_2_, na, nb_2_>), AuthTaggedSignals1.TRunning1.a.Internal)
      | Sent.(Encrypt.(PK__.(b), <nb>), <a, b_2_, na, nb_2_>) <- FACTS_TO_RENAME,
        b_2_ == b, nb_2_ == nb,
        not((member(nb,{Ne1})) or (member(nb,{Ne2})))},
      {(Sent.(Encrypt.(PK__.(b), <nb>), <a, b_2_, na, nb_2_>), AuthTaggedSignals1.TRunning1.a.External1)
      | Sent.(Encrypt.(PK__.(b), <nb>), <a, b_2_, na, nb_2_>) <- FACTS_TO_RENAME,
        b_2_ == b, nb_2_ == nb,
        member(nb,{Ne1})},
      {(Sent.(Encrypt.(PK__.(b), <nb>), <a, b_2_, na, nb_2_>), AuthTaggedSignals1.TRunning1.a.External2)
      | Sent.(Encrypt.(PK__.(b), <nb>), <a, b_2_, na, nb_2_>) <- FACTS_TO_RENAME,
        b_2_ == b, nb_2_ == nb,
        member(nb,{Ne2})}
    })

  (RenamedDeductions_,IK,RenamedLearnableFact) =
    AUTH_COMMON::RenameClosure(FACT_RENAMING,FACTS_TO_RENAME)

  -- Build the basic (non-chased) intruder by renaming all the infer.(f,fs)
  -- events corresponding to the messages that should be renamed.
  INTRUDER_0 = 
    INTRUDER_M::INTRUDER_00(RenamedDeductions,RenamedLearnableFact)
      [[infer.(AuthTaggedSignals1.TRunning1.a.tag_,fs_) <-
         signal.Running1.INITIATOR_role.a
        | (AuthTaggedSignals1.TRunning1.a.tag_, fs_) <- RenamedDeductions,
          member(a,inter(Agent,HONEST))
      ]] \ {|infer|}

exports
  RenamedDeductions = RenamedDeductions_

  -- Datatypes below are what facts are renamed in in FACT_RENAMING above
  datatype Tag1 = Internal | External1 | External2
  datatype TaggedSignals1 = TRunning1.ALL_PRINCIPALS.Tag1

  -- Specs for all agents being authenticated

  AuthenticateINITIATORToRESPONDERAliveness =
    let
      bInstance(b, nb) =
        signal.Running1?a_role_?a : inter(Agent,HONEST) ->
          RUN({signal.Commit1.RESPONDER_role.b.a})
      run(b, nb) = 
        signal.Running1?a_role_?a : inter(Agent,HONEST) -> 
        run(b, nb)
    within
      -- Number argument tuples so that duplicates are not eliminated
      ||| (_,b, nb): {(1, Bob, Ne1), (2, Bob, Ne2)} @
        sbisim(bInstance(b, nb)) ||| run(b, nb)

  -- System for authentication checking

  SYSTEM_1 =
    let Agent_renamed_ = ALGEBRA_M::applyRenamingToSet(Agent)
        Nonce_renamed_ = ALGEBRA_M::applyRenamingToSet(Nonce)
    within
      AUTH_COMMON::AUTH_SYSTEM(INTRUDER_0,IK)
       [[send.a.b.ALGEBRA_M::rmb((Msg3, Encrypt.(PK__.(b), <nb>), <na, nb>)) <-
          signal.Running1.INITIATOR_role.a,  
        receive.a.b.ALGEBRA_M::rmb((Msg3, Encrypt.(PK__.(b), <nb>), <na, nb>)) <-
          signal.Commit1.RESPONDER_role.b.a |
            a <- Agent_renamed_, b <- Agent_renamed_, na <- Nonce_renamed_, 
            nb <- Nonce_renamed_,
            member(a,HONEST),
            member((Msg3, Encrypt.(PK__.(b), <nb>), <na, nb>),SYSTEM_M::OUTPUT_MSG3),
            member((Msg3, Encrypt.(PK__.(b), <nb>), <na, nb>),SYSTEM_M::INPUT_MSG3)
      ]]
      \ {| env, leak, send, receive|}

endmodule

assert AUTH1_M::AuthenticateINITIATORToRESPONDERAliveness [T= 
       AUTH1_M::SYSTEM_1

