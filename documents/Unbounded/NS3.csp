    AUTH_COMMON::RenameClosure(FACT_RENAMING,FACTS_TO_RENAME)

  -- Build the basic (non-chased) intruder by renaming all the infer.(f,fs)
  -- events corresponding to the messages that should be renamed.
  INTRUDER_0 = 
    INTRUDER_M::INTRUDER_00(RenamedDeductions,RenamedLearnableFact)
      [[infer.(AuthTaggedSignals3.TRunning3.a.b.na.nb.tag_,fs_) <-
         signal.Running3.INITIATOR_role.a.b.na.nb
        | (AuthTaggedSignals3.TRunning3.a.b.na.nb.tag_, fs_) <- RenamedDeductions,
          member(a,inter(Agent,HONEST)), member(b, {Bob}), member(nb,{Ne1, Ne2})
      ]] \ {|infer|}

exports
  RenamedDeductions = RenamedDeductions_

  -- Datatypes below are what facts are renamed in in FACT_RENAMING above
  datatype Tag3 = Internal | External1 | External2
  datatype TaggedSignals3 = TRunning3.ALL_PRINCIPALS.ALL_PRINCIPALS.Nonce.Nonce.Tag3

  -- Specs for all agents being authenticated

  AuthenticateINITIATORToRESPONDERNonInjectiveAgreement_na_nb =
    let
      bInstance(b, nb) =
        signal.Running3.INITIATOR_role?a : inter(Agent,HONEST)!b?na : {Na1, Na2}!nb ->
          RUN({signal.Commit3.RESPONDER_role.b.a.na.nb})
      run(b, nb) = 
        signal.Running3.INITIATOR_role?a : inter(Agent,HONEST)!b?na!nb -> 
        run(b, nb)
    within
      -- Number argument tuples so that duplicates are not eliminated
      ||| (_,b, nb): {(1, Bob, Ne1), (2, Bob, Ne2)} @
        sbisim(bInstance(b, nb)) ||| run(b, nb)

  -- System for authentication checking

  SYSTEM_3 =
    let Agent_renamed_ = ALGEBRA_M::applyRenamingToSet(Agent)
        Nonce_renamed_ = ALGEBRA_M::applyRenamingToSet(Nonce)
    within
      AUTH_COMMON::AUTH_SYSTEM(INTRUDER_0,IK)
       [[send.a.b.ALGEBRA_M::rmb((Msg3, Encrypt.(PK__.(b), <nb>), <na, nb>)) <-
          signal.Running3.INITIATOR_role.a.b.na.nb,  
        receive.a.b.ALGEBRA_M::rmb((Msg3, Encrypt.(PK__.(b), <nb>), <na, nb>)) <-
          signal.Commit3.RESPONDER_role.b.a.na.nb |
            a <- Agent_renamed_, b <- Agent_renamed_, na <- Nonce_renamed_, 
            nb <- Nonce_renamed_,
            member(a,HONEST),
            member((Msg3, Encrypt.(PK__.(b), <nb>), <na, nb>),SYSTEM_M::OUTPUT_MSG3),
            member((Msg3, Encrypt.(PK__.(b), <nb>), <na, nb>),SYSTEM_M::INPUT_MSG3)
      ]]
