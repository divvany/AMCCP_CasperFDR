        Nonce_renamed_ = ALGEBRA_M::applyRenamingToSet(Nonce)
        SessionKey_renamed_ = ALGEBRA_M::applyRenamingToSet(SessionKey)
    within
      AUTH_COMMON::AUTH_SYSTEM(INTRUDER_0,IK)
       [[send.a.s.ALGEBRA_M::rmb((Msg1b, Encrypt.(ServerKey__.(a), <na, a, b>), <b>)) <-
          signal.Running2.INITIATOR_role.a.b,  
        receive.s.b.ALGEBRA_M::rmb((Msg3b, Sq.<na, Encrypt.(ServerKey__.(b), <nb, kab>)>, <a>)) <-
          signal.Commit2.RESPONDER_role.b.a |
            a <- Agent_renamed_, s <- Server_renamed_, b <- Agent_renamed_, 
