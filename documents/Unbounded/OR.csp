module SYSTEM_M

  -- types of messages sent and received by agents, as they are
  -- considered by those agents

  input_proj((l_,m_,se_,re_)) = (l_,m_,re_)
  rmb_input_proj((l_,m_,se_,re_)) = ALGEBRA_M::rmb((l_,m_,re_))
  output_proj((l_,m_,se_,re_)) = (l_,m_,se_)

  INPUT_INT_MSG :: {(Labels, Encryption, <Encryption>)}
  INPUT_INT_MSG = 
    
    Union({
      INPUT_INT_MSG1a,
      INPUT_INT_MSG1b,
      INPUT_INT_MSG2a,
      INPUT_INT_MSG2b,
      INPUT_INT_MSG3a,
      INPUT_INT_MSG3b
    })

  OUTPUT_INT_MSG :: {(Labels, Encryption, <Encryption>)}
  OUTPUT_INT_MSG = 
    
    Union({
      OUTPUT_INT_MSG1a,
      OUTPUT_INT_MSG1b,
      OUTPUT_INT_MSG2a,
      OUTPUT_INT_MSG2b,
      OUTPUT_INT_MSG3a,
      OUTPUT_INT_MSG3b
    })

  -- INITIATOR

  INITIATOR_0(runTimeRemaining_, a, s, na) =
      INITIATOR_0'(runTimeRemaining_, a, s, na)

  INITIATOR_0'(runTimeRemaining_, a, s, na) = 
    tock -> INITIATOR_0'(runTimeRemaining_, a, s, na)
    [] 
    [] b : Agent @ a != b & member((Env0, b,<>), ENV_INT_MSG0) & 
      env_I.a.(Env0, b,<>) ->
    member((Msg1a, na,<>), OUTPUT_INT_MSG1a) & 
    output.a.b.(Msg1a, na,<>) ->
    member((Msg1b, Encrypt.(ServerKey__.(a), <na, a, b>),<b>), OUTPUT_INT_MSG1b) & 
    output.a.s.(Msg1b, Encrypt.(ServerKey__.(a), <na, a, b>),<b>) ->
      INITIATOR_0''(runTimeRemaining_, a, s, na, b)

  INITIATOR_0''(runTimeRemaining_, a, s, na, b) = 
    tock -> (if runTimeRemaining_ < 0 then SKIP
      else INITIATOR_0''(runTimeRemaining_-1, a, s, na, b))
    [] 
    [] kab : SessionKey @ 
      member((Msg3a, Encrypt.(ServerKey__.(a), <na, kab>),<>), INPUT_INT_MSG3a) & 
      input.s.a.(Msg3a, Encrypt.(ServerKey__.(a), <na, kab>),<>) ->
    SKIP

  INITIATOR_1(runTimeRemaining_, a, s, na) = INITIATOR_0(runTimeRemaining_, a, s, na)

  INITIATOR(runTimeRemaining_, a, s, na) =
    INITIATOR_1(runTimeRemaining_, a, s, na)
      [[input.s.a.(l_,m_,re_) <- receive.s.a.ALGEBRA_M::rmb((l_,m_,re_)) |
          s <- Server, (l_,m_,se_,re_) <- INT_MSG_INFO3a]]
      [[output.a.b.(l_,m_,se_) <- send.a.b.ALGEBRA_M::rmb((l_,m_,se_)) |
          b <- Agent, (l_,m_,se_,re_) <- INT_MSG_INFO1a]]
      [[output.a.s.(l_,m_,se_) <- send.a.s.ALGEBRA_M::rmb((l_,m_,se_)) |
          s <- Server, (l_,m_,se_,re_) <- INT_MSG_INFO1b]]
      [[env_I.a.m_ <- env.a.ALGEBRA_M::rmb(m_) |
          m_ <- ENV_INT_MSG0]]

  -- RESPONDER

  RESPONDER_0(runTimeRemaining_, b, s, nb) =
      RESPONDER_0'(runTimeRemaining_, b, s, nb)

  RESPONDER_0'(runTimeRemaining_, b, s, nb) = 
    tock -> RESPONDER_0'(runTimeRemaining_, b, s, nb)
    [] 
    [] a : Agent @ [] na : Nonce @ a != b & 
      member((Msg1a, na,<>), INPUT_INT_MSG1a) & 
      input.a.b.(Msg1a, na,<>) ->
    member((Msg2a, na,<>), OUTPUT_INT_MSG2a) & 
    output.b.s.(Msg2a, na,<>) ->
    member((Msg2b, Encrypt.(ServerKey__.(b), <nb, na, a, b>),<>), OUTPUT_INT_MSG2b) & 
    output.b.s.(Msg2b, Encrypt.(ServerKey__.(b), <nb, na, a, b>),<>) ->
      RESPONDER_0''(runTimeRemaining_, b, s, nb, a, na)

  RESPONDER_0''(runTimeRemaining_, b, s, nb, a, na) = 
    tock -> (if runTimeRemaining_ < 0 then SKIP
      else RESPONDER_0''(runTimeRemaining_-1, b, s, nb, a, na))
    [] 
    [] kab : SessionKey @ 
      member((Msg3b, Sq.<na, Encrypt.(ServerKey__.(b), <nb, kab>)>,<a>), INPUT_INT_MSG3b) & 
      input.s.b.(Msg3b, Sq.<na, Encrypt.(ServerKey__.(b), <nb, kab>)>,<a>) ->
    SKIP

  RESPONDER_1(runTimeRemaining_, b, s, nb) = RESPONDER_0(runTimeRemaining_, b, s, nb)

  RESPONDER(runTimeRemaining_, b, s, nb) =
    RESPONDER_1(runTimeRemaining_, b, s, nb)
      [[input.a.b.(l_,m_,re_) <- receive.a.b.ALGEBRA_M::rmb((l_,m_,re_)) |
          a <- Agent, (l_,m_,se_,re_) <- INT_MSG_INFO1a]]
      [[input.s.b.(l_,m_,re_) <- receive.s.b.ALGEBRA_M::rmb((l_,m_,re_)) |
          s <- Server, (l_,m_,se_,re_) <- INT_MSG_INFO3b]]
      [[output.b.s.(l_,m_,se_) <- send.b.s.ALGEBRA_M::rmb((l_,m_,se_)) |
          s <- Server, (l_,m_,se_,re_) <- INT_MSG_INFO2a]]
      [[output.b.s.(l_,m_,se_) <- send.b.s.ALGEBRA_M::rmb((l_,m_,se_)) |
          s <- Server, (l_,m_,se_,re_) <- INT_MSG_INFO2b]]

  -- SERVER

  SERVER_0(runTimeRemaining_, s, kab) =
      SERVER_0'(runTimeRemaining_, s, kab)

  SERVER_0'(runTimeRemaining_, s, kab) = 
    tock -> SERVER_0'(runTimeRemaining_, s, kab)
    [] 
    [] a : Agent @ [] b : Agent @ [] na : Nonce @ 
      member((Msg1b, Encrypt.(ServerKey__.(a), <na, a, b>),<>), INPUT_INT_MSG1b) & 
      input.a.s.(Msg1b, Encrypt.(ServerKey__.(a), <na, a, b>),<>) ->
      SERVER_0''(runTimeRemaining_, s, kab, a, b, na)

  SERVER_0''(runTimeRemaining_, s, kab, a, b, na) = 
    tock -> (if runTimeRemaining_ < 0 then SKIP
      else SERVER_0''(runTimeRemaining_-1, s, kab, a, b, na))
    [] 
    input.b.s.(Msg2a, na,<>) ->
      SERVER_0'''(runTimeRemaining_, s, kab, a, b, na)

  SERVER_0'''(runTimeRemaining_, s, kab, a, b, na) = 
    tock -> (if runTimeRemaining_ < 0 then SKIP
      else SERVER_0'''(runTimeRemaining_-1, s, kab, a, b, na))
    [] 
    [] nb : Nonce @ 
      member((Msg2b, Encrypt.(ServerKey__.(b), <nb, na, a, b>),<>), INPUT_INT_MSG2b) & 
      input.b.s.(Msg2b, Encrypt.(ServerKey__.(b), <nb, na, a, b>),<>) ->
    member((Msg3a, Encrypt.(ServerKey__.(a), <na, kab>),<>), OUTPUT_INT_MSG3a) & 
    output.s.a.(Msg3a, Encrypt.(ServerKey__.(a), <na, kab>),<>) ->
    member((Msg3b, Sq.<na, Encrypt.(ServerKey__.(b), <nb, kab>)>,<>), OUTPUT_INT_MSG3b) & 
    output.s.b.(Msg3b, Sq.<na, Encrypt.(ServerKey__.(b), <nb, kab>)>,<>) ->
    SKIP

  SERVER_1(runTimeRemaining_, s, kab) = SERVER_0(runTimeRemaining_, s, kab)

  SERVER(runTimeRemaining_, s, kab) =
    SERVER_1(runTimeRemaining_, s, kab)
      [[input.a.s.(l_,m_,re_) <- receive.a.s.ALGEBRA_M::rmb((l_,m_,re_)) |
          a <- Agent, (l_,m_,se_,re_) <- INT_MSG_INFO1b]]
      [[input.b.s.(l_,m_,re_) <- receive.b.s.ALGEBRA_M::rmb((l_,m_,re_)) |
          b <- Agent, (l_,m_,se_,re_) <- INT_MSG_INFO2a]]
      [[input.b.s.(l_,m_,re_) <- receive.b.s.ALGEBRA_M::rmb((l_,m_,re_)) |
          b <- Agent, (l_,m_,se_,re_) <- INT_MSG_INFO2b]]
      [[output.s.a.(l_,m_,se_) <- send.s.a.ALGEBRA_M::rmb((l_,m_,se_)) |
          a <- Agent, (l_,m_,se_,re_) <- INT_MSG_INFO3a]]
      [[output.s.b.(l_,m_,se_) <- send.s.b.ALGEBRA_M::rmb((l_,m_,se_)) |
          b <- Agent, (l_,m_,se_,re_) <- INT_MSG_INFO3b]]

  -- Process representing Alice

  Alpha_Alice = 
    Union({
      {|env.Alice|},
      {|send.Alice.A_ | A_ <- ALL_PRINCIPALS|},
      {tock}
    })

  AGENT_Alice = RUN({|tock|})

  -- Process representing Bob

  Alpha_RESPONDER_Bob = 
    Union({
      {|send.Bob.A_.m_ | A_ <- ALL_PRINCIPALS, m_ <- OUTPUT_MSG2a|},
      {|send.Bob.A_.m_ | A_ <- ALL_PRINCIPALS, m_ <- OUTPUT_MSG2b|},
      {|receive.A_.Bob.m_ | A_ <- ALL_PRINCIPALS, m_ <- INPUT_MSG1a|},
      {|receive.A_.Bob.m_ | A_ <- ALL_PRINCIPALS, m_ <- INPUT_MSG3b|},
      {tock}
    })

  RESPONDER_Bob = ((RESPONDER(MaxRunTime, Bob, Sam, Ne1) ; RUN({tock}))
    [|{tock}|]
    (RESPONDER(MaxRunTime, Bob, Sam, Ne2) ; RUN({tock})))

  Alpha_Bob = 
    Union({
      {|env.Bob|},
      {|send.Bob.A_ | A_ <- ALL_PRINCIPALS|},
      {|receive.A_.Bob.m_ | A_ <- ALL_PRINCIPALS, m_ <- INPUT_MSG1a|},
      {|receive.A_.Bob.m_ | A_ <- ALL_PRINCIPALS, m_ <- INPUT_MSG3b|},
      {tock}
    })

  AGENT_Bob =
    (RESPONDER_Bob [Alpha_RESPONDER_Bob || {} ] STOP)

  -- Process representing Sam

  Alpha_Sam = 
    Union({
      {|env.Sam|},
      {|send.Sam.A_ | A_ <- ALL_PRINCIPALS|},
      {tock}
    })

  AGENT_Sam = RUN({|tock|})

