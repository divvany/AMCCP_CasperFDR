            member((Msg1b, Encrypt.(ServerKey__.(a), <na, a, b>), <b>),SYSTEM_M::OUTPUT_MSG1b),
            member((Msg3b, Sq.<na, Encrypt.(ServerKey__.(b), <nb, kab>)>, <a>),SYSTEM_M::INPUT_MSG3b)
      ]]
      \ {| env, leak, send, receive|}

endmodule

